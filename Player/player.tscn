script/source = "extends CharacterBody2D

const SPEED = 300.0
const JUMP_VELOCITY = -400.0
var health = 100
var gold = 0

@onready var anim = $AnimatedSprite2D #можно написать по другому get_node(\"AnimatedSprite2D\")

func _physics_process(delta: float) -> void:
	# Add the gravity.
	if not is_on_floor():
		velocity += get_gravity() * delta #delta переменная которая зависит от мощиности пк

	# Handle jump.
	if Input.is_action_just_pressed(\"jump\") and is_on_floor(): #Input.is_action_just_pressed(\"ui_accept\") and is_on_floor() фиксирует нажатие на пробел когда игрок на полу
		velocity.y = JUMP_VELOCITY
		anim.play(\"Jump\")

	# Get the input direction and handle the movement/deceleration.
	# As good practice, you should replace UI actions with custom gameplay actions.
	var direction := Input.get_axis(\"left\", \"right\") #Input.get_axis(\"ui_left\", \"ui_right\") получает ось -1 или 1 при нажетие стрелок лево или право
	if direction:
		velocity.x = direction * SPEED
		if velocity.y == 0:
			anim.play(\"Run\")
	else:
		velocity.x = move_toward(velocity.x, 0, SPEED) #move_toward(velocity.x, 0, SPEED) скорость стремиться к 0
		if velocity.y == 0:
			anim.play(\"Idel\")
	if direction == -1:
		$AnimatedSprite2D.flip_h = true
	elif direction == 1:
		$AnimatedSprite2D.flip_h = false
		
	if velocity.y > 0:
		anim.play(\"Fall\")

	if health <= 0:
		queue_free()
		get_tree().change_scene_to_file(\"res://menu.tscn\")

	move_and_slide()
"

